#lang racket
(define G
  (位 (i)
    (cond
      ((zero? i)
       (位 (n m)
         (cond
           ((zero? m) n)
           (else (add1 ((G 0) n (sub1 m)))))))
      ((zero? (sub1 i))
       (位 (n m)
         (cond
           ((zero? m) 0)
           (else ((G 0) n ((G 1) n (sub1 m)))))))
      (else
       (位 (n m)
         (cond
           ((zero? m) 1)
           (else
            ((G (sub1 i)) n ((G i) n (sub1 m))))))))))

